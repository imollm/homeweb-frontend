{
  "baseUrl": "http://localhost/homeweb-backend/public/api",
  "headers": {
    "authorization": "Bearer %s",
    "accept": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": {
        "uri": "/auth/register",
        "method": "POST",
        "auth": false
      },
      "login": {
        "uri": "/auth/login",
        "method": "POST",
        "auth": false
      },
      "logout": {
        "uri": "/auth/logout",
        "method": "GET",
        "auth": false
      },
      "user": {
        "uri": "/auth/user",
        "method": "GET",
        "auth": false
      }
    },
    "properties": {
      "index": {
        "uri": "/properties/index",
        "method": "GET",
        "auth": false
      },
      "create": {
        "uri": "/properties/create",
        "method": "POST",
        "auth": true
      },
      "showByFilter": {
        "uri": "/properties/showByFilter",
        "method": "POST",
        "auth": false
      },
      "owner": {
        "uri": "/properties/%s/owner",
        "method": "GET",
        "auth": true
      },
      "setActive": {
        "uri": "/properties/%s/setActive",
        "method": "GET",
        "auth": true
      },
      "showById": {
        "uri": "/properties/%s/show",
        "method": "GET",
        "auth": false
      },
      "update": {
        "uri": "/properties/%s/update",
        "method": "PUT",
        "auth": true
      },
      "last": {
        "uri": "/properties/last",
        "method": "GET",
        "auth": false
      }
    },
    "roles": {
      "user": {
        "uri": "/roles/%s",
        "method": "GET",
        "auth": true
      },
      "myRole": {
        "uri": "/roles/myRole",
        "method": "GET",
        "auth": true
      },
      "all": {
        "uri": "/roles/all",
        "method": "GET",
        "auth": true
      }
    },
    "categories": {
      "index": {
        "uri": "/categories/index",
        "method": "GET",
        "auth": false
      },
      "create": {
        "uri": "/categories/create",
        "method": "POST",
        "auth": true
      },
      "update": {
        "uri": "/%s/update",
        "method": "GET",
        "auth": false
      },
      "propertiesByCategoryName": {
        "uri": "/categories/name/%s/properties",
        "method": "GET",
        "auth": false
      },
      "propertiesByCategoryId": {
        "uri": "/categories/id/%s/properties",
        "method": "GET",
        "auth": false
      }
    },
    "rangePrice": {
      "index": {
        "uri": "/rangePrice/index",
        "method": "GET",
        "auth": false
      }
    },
    "cities": {
      "index": {
        "uri": "/cities/index",
        "method": "GET",
        "auth": false
      }
    },
    "contact": {
      "send": {
        "uri": "/contact/send",
        "method": "GET",
        "auth": false
      }
    },
    "image": {
      "categories": {
        "uri": "/image/categories/%s",
        "method": "GET",
        "auth": false
      },
      "properties": {
        "uri": "/image/properties/%s",
        "method": "GET",
        "auth": false
      }
    },
    "sales": {
      "index": {
        "uri": "/sales/index",
        "method": "GET",
        "auth": true
      },
      "actualYear": {
        "uri": "/sales/actualYear",
        "method": "GET",
        "auth": true
      }
    }
  }
}
